{
  "npm.enableScriptExplorer": true,
  /* Formats */
  "files.associations": {
    "*.pcss": "postcss"
  },
  /* Linters */
  "editor.formatOnSave": true,
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "eslint.validate": ["javascript", { "language": "vue", "autoFix": true }],
  "eslint.autoFixOnSave": true
}
