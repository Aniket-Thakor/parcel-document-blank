{
  "name": "parcel-template-blank",
  "version": "1.1.0",
  "description": "Blank template project starter-kit with Parcel Bundler",
  "main": "src/index.html",
  "scripts": {
    "dev": "parcel serve $npm_package_main",
    "watch": "parcel watch $npm_package_main",
    "lint:js": "eslint src/js/**/*.js",
    "lint:css": "stylelint src/css/**/*.css",
    "lint": "npm-run-all lint:*",
    "clean:cache": "rm -rf .cache dist build",
    "clean:all": "rm -rf node_modules package-lock.json .cache dist build",
    "build": "rm -rf build/* && parcel build $npm_package_main -d build --public-url /",
    "build:gh-pages": "rm -rf docs/* && parcel build $npm_package_main -d docs --public-url /$npm_package_name/",
    "deploy:gh-pages": "git subtree push --prefix docs origin gh-pages"
  },
  "keywords": [
    "parcel",
    "template",
    "html5",
    "css3",
    "postcss",
    "stylelint",
    "javascript",
    "eslint"
  ],
  "author": "Manz",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "htmlhint": "^0.11.0",
    "npm-run-all": "^4.1.3",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "postcss-clean": "^1.1.0",
    "postcss-font-magician": "^2.2.2",
    "postcss-mixins": "^6.2.2",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.18.0",
    "stylelint": "^13.2.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-standard": "^20.0.0"
  },
  "dependencies": {}
}
